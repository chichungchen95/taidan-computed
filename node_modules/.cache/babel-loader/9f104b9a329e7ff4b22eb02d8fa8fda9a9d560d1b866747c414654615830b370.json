\n    // 計算電費\n    duocap() {\n      if(this.duoey === 1.68){\n        this.duocadd = this.taidal[0].price * this.duomon;\n      } else if(this.duoey ===2.45){\n        this.duocadd = this.taidal[1].price * this.duomon;\n      } else if(this.duoey === 3.7){\n        this.duocadd = this.taidal[2].price * this.duomon;\n      } else if(this.duoey === 5.04){\n        this.duocadd = this.taidal[3].price * this.duomon;\n      } else if(this.duoey === 6.24){\n        this.duocadd = this.taidal[4].price * this.duomon;\n      } else if(this.duoey === 8.46){\n        this.duocadd = this.taidal[5].price * this.duomon;\n      }\n    },\n  },\n}\n</script>\n\n<style>\n</style>\n"],"mappings":"AAYA,eAAe;EACbA,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,CAAC;MACPC,IAAI,EAAE,CAAC;MACPC,KAAK,EAAE,CAAC;MACRC,OAAO,EAAE,CAAC;MACVC,MAAM,EACJ,CACE;QAAEN,IAAI,EAAE,IAAI;QAAEO,KAAK,EAAE;MAAK,CAAC,EAC3B;QAAEP,IAAI,EAAE,IAAI;QAAEO,KAAK,EAAE,IAAI,GAAC;MAAK,CAAC,EAChC;QAAEP,IAAI,EAAE,IAAI;QAAEO,KAAK,EAAE,GAAG,GAAC;MAAK,CAAC,EAC/B;QAAEP,IAAI,EAAE,IAAI;QAAEO,KAAK,EAAE,IAAI,GAAC;MAAI,CAAC,EAC/B;QAAEP,IAAI,EAAE,IAAI;QAAEO,KAAK,EAAE,IAAI,GAAC;MAAK,CAAC,EAChC;QAAEP,IAAI,EAAE,IAAI;QAAEO,KAAK,EAAE,IAAI,GAAC;MAAK,CAAC;IAEtC;EACF,CAAC;EACDC,KAAK,EAAE;IACL;IACAN,IAAIA,CAAA,EAAG;MACL,IAAI,CAACO,OAAO,CAAC,CAAC;MACd,IAAI,CAACC,MAAM,CAAC,CAAC;IACf,CAAC;IACD;IACAP,IAAIA,CAAA,EAAG;MACL,IAAI,CAACM,OAAO,CAAC,CAAC;MACd,IAAI,CAACC,MAAM,CAAC,CAAC;IACf;EACF,CAAC;EACDC,QAAQ,EAAE;IACR;IACAC,GAAGA,CAAA,EAAG;MACJ,OAAQ,IAAI,CAACV,IAAG,GAAI,IAAI,CAACC,IAAI,GAAI,IAAI;IACvC,CAAC;IACDU,KAAKA,CAAA,EAAG;MACN,OAAQ,IAAI,CAACC,MAAK,GAAI,IAAI,CAACV,KAAK;IAClC,CAAC;IACD;IACAU,MAAMA,CAAA,EAAG;MACP,OAAO,IAAI,CAACF,GAAE,GAAI,EAAE;IACtB;EAEF,CAAC;EACDG,OAAO,EAAE;IAEP;IACAN,OAAOA,CAAA,EAAG;MACR,IAAI,IAAI,CAACK,MAAK,GAAG,GAAG,EAAE;QACpB,OAAO,IAAI,CAACV,KAAI,GAAI,IAAI;MAC1B,OAAO,IAAI,IAAI,CAACU,MAAK,GAAI,GAAG,EAAE;QAC5B,OAAO,IAAI,CAACV,KAAI,GAAI,IAAI;MAC1B,OAAO,IAAI,IAAI,CAACU,MAAK,GAAI,GAAG,EAAE;QAC5B,OAAO,IAAI,CAACV,KA